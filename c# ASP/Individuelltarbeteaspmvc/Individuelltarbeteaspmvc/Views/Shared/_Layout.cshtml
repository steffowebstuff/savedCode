<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <!-- Put link to js and css file here-->
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script><!-- Bör vara den här som fixar ajaxrequesterna-->
    <script src="http://maps.googleapis.com/maps/api/js?sensor=true"></script>
    
</head>
<body onload = "initialize()">
    <!--<body>-->
    <div class="page">
        <header>
            <div id="title">
                <h1>My MVC Application</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <nav>
                <ul id="menu">
                    <li class="login">@Html.ActionLink("Vädertjänst", "Index", "Home")</li>
                  <!--  <li class="login">@Html.ActionLink("About", "About", "Home")</li> -->
                  <!--  <li class="login">@Html.ActionLink("New Link", "About", "Home")</li> -->
                    <li> @Html.ActionLink("Log On with Twitter", "LogOnTwitter", "Account")</li>
                    <li> @Html.ActionLink("To New Weather", "FindCity", "NewWeather")</li>
                    <li> @Html.ActionLink("To New User", "Index", "NewUser")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
            
        </section>
        <footer>
            
        </footer>
    </div>
    @RenderSection("ScriptBodyEnd", false)
</body>
</html>
